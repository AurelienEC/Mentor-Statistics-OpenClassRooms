{
  "manifest_version": 2,
  "name": "Mentor Statistics OpenClassRooms",
  "version": "1.0",
  "description": "A web extension for OpenClassRooms mentors that gives you access to more statistics on your session history.",
  "permissions": ["tabs"],
  "content_scripts": [
    {
      "matches": [
        "https://openclassrooms.com/*mentorship/dashboard/mentorship-sessions-history"
      ],
      "js": [
        "libs/jquery-3.3.1.js",
        "libs/jquery.tabletojson.js",
        "config/config.js",
        "src/DateUtils.js",
        "src/Price.js",
        "src/Statistics.js",
        "src/Display.js",
        "src/Parser.js",
        "src/main.js"
      ]
    }
  ]
}
